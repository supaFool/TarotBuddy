                                                        <Lighting diffuseConstant="1.5714285714285714"
                                                                  specularConstant="1.2365079365079366"
                                                                  specularExponent="19.047619047619047">
                                                            <bumpInput>
                                                                <Shadow/>
                                                            </bumpInput>
                                                            <light>
                                                                <javafx.scene.effect.Light.Distant/>
