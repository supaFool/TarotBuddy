                                                        <Lighting diffuseConstant="1.1428571428571428"
                                                                  specularConstant="1.2523809523809524"
                                                                  specularExponent="19.365079365079364"
                                                                  surfaceScale="2.373015873015873">
                                                            <bumpInput>
                                                                <Shadow/>
                                                            </bumpInput>
                                                            <light>
                                                                <javafx.scene.effect.Light.Distant color="#ffe799"/>
                                                            </light>
                                                        </Lighting>
                                                    </effect>
                                                    <font>
                                                        <Font name="Segoe Script" size="12.0" fx:id="x2"/>
                                                    </font>
                                                </Button>
                                                <Button font="$x2" mnemonicParsing="false" onAction="#cancel"
